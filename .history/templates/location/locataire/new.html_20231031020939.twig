                        <div class="card card-xl-stretch mb-xl-8">
                            <!--begin::Header-->
                            <div class="card-header border-0 pt-5">
                                <h3 class="card-title align-items-start flex-column">
                                    <span class="card-label fw-bold fs-3 mb-1">
                                        <font style="vertical-align: inherit;">
                                            <font style="vertical-align: inherit;">Famille</font>
                                        </font>
                                    </span>
                                </h3>
                
                            </div>
                            <!--end::Header-->
                
                            <!--begin::Body-->
                            <div class="card-body py-3">
                                <!--begin::Table container-->
                
                                <div class="row">
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                            Ethnie
                                        </label>
                                        {{ form_widget(form.Ethnie) }}
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                        Nombre enfants
                                        </label>
                                        {{ form_widget(form.NbEnfts) }}
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                        Nombre personnes á charge
                                        </label>
                                    {{ form_widget(form.NbPersChge) }}
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                            Père
                                        </label>
                                        {{ form_widget(form.Pere) }}
                                       
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                        Mère
                                        </label>
                                        {{ form_widget(form.Mere) }}
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                        Situation matrimoniale
                                        </label>
                                        {{ form_widget(form.situationMatri) }}
                                    </div>

                
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--begin::Col-->
                    <div class="col-xl-6">
                
                        <!--begin::Tables Widget 3-->
                        <div class="card card-xl-stretch mb-xl-8">
                            <!--begin::Header-->
                            <div class="card-header border-0 pt-5">
                                <h3 class="card-title align-items-start flex-column">
                                    <span class="card-label fw-bold fs-3 mb-1">
                                        <font style="vertical-align: inherit;">
                                            <font style="vertical-align: inherit;">Conjoint(e) / Colocataire</font>
                                        </font>
                                    </span>
                                </h3>
                
                            </div>
                            <!--end::Header-->
                
                            <!--begin::Body-->
                            <div class="card-body py-3">
                
               
                ->add('NPConjointe', TextType::class, [
                'label' => 'NPConjointe',
                //'attr' => ['placeholder' => 'Nom du chef de délégation']
                ])
                
                ->add('ProfConj', TextType::class, [
                'label' => 'Profession Conjointe',
                //'attr' => ['placeholder' => 'Nom du chef de délégation']
                ])
                
                ->add('', TextType::class, [
                'label' => 'Ethnie Conjointe',
                //'attr' => ['placeholder' => 'Nom du chef de délégation']
                ])
                
                
                ->add('ContactConj', TextType::class, [
                'label' => 'Contact Conjointe',
                //'attr' => ['placeholder' => 'Nom du chef de délégation']
                ])

                
               
                                <!--begin::Table container-->
                                <div class="row">
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                        Vivez avec
                                        </label>
                                        {{ form_widget(form.NPrenoms) }}
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                            Nom et Prénoms
                                        </label>
                                        {{ form_widget(form.NPConjointe) }}
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                            Profesion
                                        </label>
                                        {{ form_widget(form.LieuNaiss) }}
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                        Ethnie
                                        </label>
                                        {{ form_widget(form.Profession) }}
                                    </div>
                                    <div class="form-group col-md-12">
                                        <label class="sr-onl" for="userinput1">
                                            Contacts
                                        </label>
                                        {{ form_widget(form.addresse) }}
                                    </div>
                                    
                
                                    
                
                
                                </div>
                            </div>
                
                            <!--begin::Col-->
                        </div>
                    </div>
                
                
                
                </div>
            </div>
           
        </div>
    </div>
    <div class="modal-footer">
        {# {{ include('_includes/ajax/loader.html.twig') }} #}
        <button type="button" class="btn btn-default btn-sm" data-bs-dismiss="modal">Annuler</button>
        <button type="submit" class="btn btn-main btn-ajax btn-sm"><span class="spinner-border d-none  spinner-ajax spinner-border-sm" role="status" aria-hidden="true"></span> Valider</button>
    </div>
    {{ form_end(form) }}
{% endblock %}

{% block java %}
    <script>
        $(function () {
            init_select2('select');
        });
        $(function () {
            init_select2('select', null, '.content-doc');
        });
    </script>
{% endblock %}